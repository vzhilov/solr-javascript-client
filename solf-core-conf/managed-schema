  </fieldType>

  <fieldType name="text_nc" class="solr.TextField" positionIncrementGap="100">
    <analyzer>
      <tokenizer class="solr.StandardTokenizerFactory"/>
      <filter class="solr.LowerCaseFilterFactory"/>
    </analyzer>
  </fieldType>

  <fieldType name="text_sg" class="solr.TextField" positionIncrementGap="100">
    <analyzer>
      <tokenizer class="solr.StandardTokenizerFactory"/>
      <filter class="solr.StopFilterFactory" words="lang/stopwords_en.txt" ignoreCase="true"/>
      <filter class="solr.StopFilterFactory" format="snowball" words="lang/stopwords_ru.txt" ignoreCase="true"/>
      <filter class="solr.LowerCaseFilterFactory"/>
    </analyzer>
  </fieldType>

  <fieldType name="text_ng" class="solr.TextField" positionIncrementGap="100">
    <analyzer>
      <tokenizer class="solr.StandardTokenizerFactory"/>
      <filter class="solr.StopFilterFactory" words="stopwords.txt" ignoreCase="true"/>
      <filter class="solr.LowerCaseFilterFactory"/>
      <filter class="solr.NGramFilterFactory" minGramSize="3" maxGramSize="30"/>
    </analyzer>
  </fieldType>

  <fieldType name="text_sh" class="solr.TextField" positionIncrementGap="100">
    <analyzer>
      <tokenizer class="solr.StandardTokenizerFactory"/>
<!--      <filter class="solr.LengthFilterFactory" min="1" max="15"/>  
      <filter class="solr.TrimFilterFactory"/>-->
      <filter class="solr.ShingleFilterFactory" maxShingleSize="5" outputUnigrams="false"/>
    </analyzer>
  </fieldType>

  <fieldType name="suggestTypeLc" class="solr.TextField" positionIncrementGap="100">
    <analyzer>
      <!---->
      <charFilter class="solr.PatternReplaceCharFilterFactory" pattern="[^a-zA-Zа-яА-ЯёЁ0-9]" replacement=" "/>
      <tokenizer class="solr.StandardTokenizerFactory"/>
      <!-- <tokenizer class="solr.KeywordTokenizerFactory"/> -->
      <filter class="solr.LengthFilterFactory" min="1" max="20"/>
      <filter class="solr.TrimFilterFactory"/>            
      <!--
      <filter class="solr.ShingleFilterFactory" maxShingleSize="5" outputUnigrams="true"/> -->
      <filter class="solr.LowerCaseFilterFactory"/>
    </analyzer>
  </fieldType>

